<Application x:Class="Wpf_OnlineRestaurantSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Wpf_OnlineRestaurantSystem"
             xmlns:conv="clr-namespace:Wpf_OnlineRestaurantSystem.Converters"
             StartupUri="Views/StartWindow.xaml">
    <Application.Resources>
        <conv:StringNullOrEmptyToVisibilityConverter x:Key="StringNullOrEmptyToVisibilityConverter" />
        <conv:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <conv:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
        <conv:BoolToOpacityConverter x:Key="BoolToOpacityConverter"/>
        <conv:InverseBoolToVisibilityConverter x:Key="InverseBoolToVisibilityConverter"/>
        <conv:PasswordBoxEmptyToVisibilityConverter x:Key="PasswordBoxEmptyToVisibilityConverter"/>
        <conv:QuantityDisplayConverter x:Key="QuantityDisplayConverter"/>
        <conv:StockStatusConverter x:Key="StockStatusConverter"/>
        <conv:CancelOrderInfoConverter x:Key="CancelOrderInfoConverter"/>
        <conv:OrderStatusUpdateConverter x:Key="OrderStatusUpdateConverter" />
        <conv:TextToBackgroundConverter x:Key="TextToBackgroundConverter" />
        <conv:CollectionToVisibilityConverter x:Key="CollectionToVisibilityConverter" />
        <conv:DoubleGreaterThanZeroToVisibilityConverter x:Key="DoubleGreaterThanZeroToVisibilityConverter" />
    </Application.Resources>
</Application>
